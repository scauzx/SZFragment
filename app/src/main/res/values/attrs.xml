<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="pstsIndicatorColor" format="color" />
        <attr name="pstsUnderlineColor" format="color" />
        <attr name="pstsDividerColor" format="color" />
        <attr name="pstsIndicatorHeight" format="dimension" />
        <attr name="pstsUnderlineHeight" format="dimension" />
        <attr name="pstsDividerPadding" format="dimension" />
        <attr name="pstsTabPaddingLeftRight" format="dimension" />
        <attr name="pstsScrollOffset" format="dimension" />
        <attr name="pstsTabBackground" format="reference" />
        <attr name="pstsShouldExpand" format="boolean" />
        <attr name="pstsTextAllCaps" format="boolean" />
        <attr name="pstsFixedIndicatorWidth" format="dimension"/>
        <attr name="pstsRoundIndicator" format="boolean"/>
        <attr name="pstsCustomIndicator" format="reference"/>
        <attr name="pstsIndicatorOffset" format="dimension"/>
        <attr name="pstsIndicatorPaddingBottom" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="lineColor" format="color" />
        <attr name="lineSize" format="dimension" />
        <attr name="needShader" format="boolean"></attr>
        <attr name="adjustSize" format="boolean"></attr>
    </declare-styleable>

    <declare-styleable name="CustomViewPager">
        <attr name="pagingEnable" format="boolean" />
        <attr name="animateSwitchEnable" format="boolean" />

    </declare-styleable>

    <declare-styleable name="RoundCornerTextView">
        <attr name="tvNormalGradientColors" format="string"/>
        <attr name="tvNormalColor" format="color"/>
        <attr name="tvPressGradientColors" format="string"/>
        <attr name="tvPressColor" format="color"/>
        <attr name="tvDisEnableGradientColors" format="string"/>
        <attr name="tvDisEnableColor" format="color"/>
        <attr name="tvRadius" format="dimension"/>
        <attr name="tvNormalGradientAgent">
            <enum name="BL_TR" value="0" />
            <enum name="BOTTOM_TOP" value="1" />
            <enum name="BR_TL" value="2" />
            <enum name="LEFT_RIGHT" value="3" />
            <enum name="RIGHT_LEFT" value="4" />
            <enum name="TL_BR" value="5" />
            <enum name="TOP_BOTTOM" value="6" />
            <enum name="TR_BL" value="7" />
        </attr>
        <attr name="tvPressGradientAgent" >
            <enum name="BL_TR" value="0" />
            <enum name="BOTTOM_TOP" value="1" />
            <enum name="BR_TL" value="2" />
            <enum name="LEFT_RIGHT" value="3" />
            <enum name="RIGHT_LEFT" value="4" />
            <enum name="TL_BR" value="5" />
            <enum name="TOP_BOTTOM" value="6" />
            <enum name="TR_BL" value="7" />
        </attr>
        <attr name="tvDisEnableGradientAgent" >
            <enum name="BL_TR" value="0" />
            <enum name="BOTTOM_TOP" value="1" />
            <enum name="BR_TL" value="2" />
            <enum name="LEFT_RIGHT" value="3" />
            <enum name="RIGHT_LEFT" value="4" />
            <enum name="TL_BR" value="5" />
            <enum name="TOP_BOTTOM" value="6" />
            <enum name="TR_BL" value="7" />
        </attr>
    </declare-styleable>

</resources>